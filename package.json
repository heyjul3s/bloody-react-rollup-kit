{
  "name": "bloody-react-rollup-package-kit",
  "version": "0.0.0-semantically-released",
  "author": "Julian Low",
  "description": "A rollup starter kit for developing React packages",
  "license": "MIT",
  "scripts": {
    "docz-dev": "docz dev",
    "docz-build": "docz build",
    "commit": "npx git-cz",
    "build": "rollup  --config config/rollup.config.js",
    "bundlesize": "bundlesize --config config/bundlesize.config.js",
    "release": "yarn build && changeset release",
    "test": "jest --config=config/jest.config.js",
    "test-watch": "yarn run test-jest --watch",
    "precommit": "lint-staged --config config/lint-staged.config.js"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "peerDependencies": {
    "react": ">=16.10.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.8.4",
    "@testing-library/react": "^9.4.0",
    "@types/jest": "^25.1.2",
    "@types/react": "^16.9.19",
    "@types/testing-library__react": "^9.1.2",
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.19.2",
    "@welldone-software/why-did-you-render": "^4.0.3",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "bundlesize": "^0.18.0",
    "codesandboxer": "^1.0.3",
    "cz-conventional-changelog": "^3.1.0",
    "docz": "^2.2.0",
    "eslint": "^6.8.0",
    "jest": "^25.1.0",
    "jest-cli": "^25.1.0",
    "jest-cucumber": "^2.0.11",
    "jest-in-case": "^1.0.2",
    "lint-staged": "^10.0.7",
    "react": "^16.12.0",
    "react-codesandboxer": "^3.1.5",
    "react-dom": "^16.12.0",
    "rollup": "^1.31.1",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-typescript2": "^0.26.0",
    "rollup-plugin-uglify": "^6.0.4",
    "semantic-release": "^17.0.3",
    "ts-jest": "^25.2.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.5",
    "utility-types": "^3.10.0"
  },
  "dependencies": {}
}
