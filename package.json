{
  "name": "bloody-react-rollup-package-kit",
  "version": "0.0.0-semantically-released",
  "author": "Julian Low",
  "description": "A rollup starter kit for developing React packages",
  "license": "MIT",
  "scripts": {
    "docz-dev": "docz dev",
    "docz-build": "docz build",
    "commit": "npx git-cz",
    "build": "rollup  --config config/roll.config.js",
    "bundlesize": "bundlesize --config configs/bundlesize.config.js",
    "release": "yarn build && changeset release",
    "test": "jest --config=config/jest-config.js",
    "test-watch": "yarn run test-jest --watch",
    "precommit": "lint-staged"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "peerDependencies": {
    "react": ">=16.10.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.6.2",
    "@testing-library/react": "^9.3.0",
    "@types/jest": "^24.0.18",
    "@types/react": "^16.9.5",
    "@types/testing-library__react": "^9.1.1",
    "@typescript-eslint/eslint-plugin": "^2.3.2",
    "@typescript-eslint/parser": "^2.3.2",
    "@welldone-software/why-did-you-render": "^3.3.6",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "bundlesize": "^0.18.0",
    "codesandboxer": "^1.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "docz": "^2.0.0-rc.46",
    "eslint": "^6.5.1",
    "jest": "^24.9.0",
    "jest-cli": "^24.9.0",
    "jest-cucumber": "^2.0.11",
    "jest-in-case": "^1.0.2",
    "lint-staged": "^9.4.1",
    "react": "^16.10.2",
    "react-codesandboxer": "^3.1.5",
    "react-dom": "^16.10.2",
    "rollup": "^1.23.1",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-typescript2": "^0.24.3",
    "rollup-plugin-uglify": "^6.0.3",
    "semantic-release": "^15.13.24",
    "ts-loader": "^6.2.0",
    "typescript": "^3.6.3",
    "utility-types": "^3.8.0"
  },
  "dependencies": {}
}
